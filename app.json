{
  "expo": {
    "name": "WorldOfTanksStatistics",
    "slug": "WorldOfTanksStatistics",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/world_of_tanks_icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "Mobile app for monitoring personal statistics in World Of Tanks "
  }
}
